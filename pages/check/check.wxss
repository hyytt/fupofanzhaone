.body{
display: flex;
flex-direction:column;
height: 100vh; 
}
.head 
{
  background-color: darkgray;}

.list-wrap{
  flex-wrap:wrap; 
  flex-grow: 1; 
  position: relative;
}

.list{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.list-scroll-view{
  display: flex;
  flex-direction:column;
  flex-wrap:nowrap;


  }
  .view-item{
    padding-left: 3%;
    padding-right: 3%;
    padding-top: 3%;
    padding-bottom: 10%;
  }
  .text-name{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  }
  .text-photo{
    width: 80px;
    height: 130px;
    border-radius: 5%;
  }